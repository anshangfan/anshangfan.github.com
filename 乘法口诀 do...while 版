public class test05 {
		public static void main(String[] args) {
     int x=1,y=1;
		        do{
		            y=1;
		            System.out.println();
		            do{
		                System.out.print("  "+y+"*"+x+"="+(x*y));
		                y++;
		            } while(y<=x);
		            x++;
		        } while(x<10);
				
			}
		}

